#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include<string.h>
using namespace std;


int main() {
    int n,i,j,len;
    char str[10000];
    scanf("%d\n",&n);
    for(i=0;i<n;i++)
    {
       
          scanf("%s",str);
          len=strlen(str);
          for(j=0;j<len;j++)
        
        {
            if(j%2==0)
            {
                printf("%c",str[j]);
                
            }
            printf("");
        }
        printf(" ");
        for(j=0;j<len;j++)
        {
             if(j%2!=0)
            {
                printf("%c",str[j]);
            }
            
        }
        printf("\n");
    }
}


    

