#include<stdio.h>
int main()
{
    int arr[100],k,i,n,e=100,j;
    scanf("%d %d",&n,&k);
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    
    for( i=0;i<n;i=i+k)
    {
        if(arr[i]==1)
        {
            e=e-3;
        }
        else
        {
            e=e-1;
        }
    }
    printf("%d",e);
}
