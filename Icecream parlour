#include<stdio.h>
int main()
{
    int n,m,i,j,s,t,k,l,cost[10000];
    scanf("%d",&t);
    for(k=0;k<t;k++)
    {
        
        scanf("%d",&m);
        scanf("%d",&n);
        for(i=1;i<=n;i++)
        {
            scanf("%d",&cost[i]);
        }
        for(l=1;l<=n;l++)
        {
            for(j=l+1;j<=n;j++)
            {
                if(cost[l]+cost[j]==m)
                {
                    printf("%d %d\n",l,j);
                    
                }
                
            }
        }
        
    
    }
    
}


